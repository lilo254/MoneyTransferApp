    "node": "6.5.0"
  },
  "description": "Component Based Angular 1.5 App",
  "scripts": {
    "build": "./node_modules/.bin/gulp build",
    "start": "npm build && concurrent \"npm run watch\" \"npm run serve\"",
    "serve": "live-server --open=./dist --mount=scripts:./dist/scripts/",
    "watch": "./node_modules/.bin/gulp watch",
    "watch:test": "./node_modules/.bin/gulp watch:test",
    "typings": "./node_modules/.bin/tsd install",
    "postinstall": "concurrent \"npm run typings\" \"npm run build\" ",
    "test": "./node_modules/.bin/gulp test"
  },
  "author": "Keith Stewart",
  "devDependencies": {
    "@types/angular": "^1.6.4",
