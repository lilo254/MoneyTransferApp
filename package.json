    "@types/jasmine": "^2.5.41",
    "@types/jquery": "^2.0.40",
    "@types/node": "^7.0.5",
    "@types/orchestrator": "0.0.30",
    "@types/q": "0.0.32",
    "angular-mocks": "^1.5.8",
    "concurrently": "~2.2.0",
    "del": "^2.2.1",
    "gulp": "~3.9.1",
    "gulp-if": "^2.0.1",
    "gulp-inject": "^4.1.0",
    "gulp-load-plugins": "^1.2.4",
